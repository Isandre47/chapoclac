<!DOCTYPE html>

<title>Youtube</title>

{% block body %}
    <h1>Youtube</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ youtube.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ youtube.title }}</td>
            </tr>
            <tr>
                <th>SubTitle</th>
                <td>{{ youtube.subTitle }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ youtube.link }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('youtube_index') }}">back to list</a>

    <a href="{{ path('youtube_edit', {'id': youtube.id}) }}">edit</a>

    {{ include('youtube/_delete_form.html.twig') }}
{% endblock %}
