{% extends 'admin/baseAdmin.html.twig' %}

{% block body %}
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="overview-wrap">
                            <h2 class="title-1">{{ youtube.title|upper }}</h2>
                        </div>
                    </div>
                </div>
                <div class="row m-t-25">
                    <div class="col-lg-12">
                        <p>{{ youtube.subTitle }}</p>
                    </div>
                </div>
                <div class="container-contact100-form-btn">
                    <div class="wrap-contact100-form-btn">
                        <div class="contact100-form-bgbtn"></div>
                        <a href="{{ path('youtube_edit', {'id': youtube.id}) }}"><button class="contact100-form-btn">
                                Modifier
                            </button></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <a href="{{ path('youtube_index') }}">back to list</a>

{% endblock %}
    {#{{ include('youtube/_form.html.twig') }}#}
